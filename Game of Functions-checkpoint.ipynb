   "source": [
    "# Write a Python function to sum all the numbers in a list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "61ae1c4a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "20\n"
     ]
    }
   ],
   "source": [
    "def sum(numbers):\n",
    "\n",
    "   total = 0\n",
    "\n",
    "   for x in numbers:\n",
    "\n",
    "       total += x\n",
    "\n",
    "   return total\n",
    "\n",
    "print(sum((8, 2, 3, 0, 7)))\n",
    "\n"
   ]
  },
